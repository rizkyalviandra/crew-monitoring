{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react"],
  "jsRules": {},
  "rules": {
    "semicolon": {
      "options": "never"
    },
    "jsx-curly-spacing": {
      "options": "never"
    },
    "jsx-equals-spacing": {
      "options": "never"
    },
    "jsx-key": true,
    "jsx-self-close": true,
    "jsx-space-before-trailing-slash": true,
    "jsx-wrap-multiline": true,
    "jsx-no-lambda": true,
    "jsx-no-multiline-js": true,
    "jsx-alignment": true
  },
  "rulesDirectory": []
}
